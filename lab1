#include <stdio.h>

int main() {
    char str[20]; //Объявляем переменную str
    int figure; //Объявляем переменную figure
    scanf("%s", &str); //Вводим текст и присваиваем его на переменную
    scanf("%d", &figure); //Вводим число и присваиваем его на переменную
    printf("%s", str); // Выводим строку
    printf("%d", figure); // Выводим число
    return 0;
}
